@{
    ViewBag.Title = "Contact US";
    ViewBag.activecontact = "active";


}
@{ Layout = "~/Views/Shared/_Layout1.cshtml";
}
<div class="super_container">
    @Html.Partial("BlackSection");

    @* Contact Us  *@

    <div class="contact-he75">
        <div class="container">
            <div class="contact-content">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="contact-inputs">
                            <h2 class="wow bounceInLeft">send <span>massage</span></h2>
                            <form action="#" method="post">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="form-group wow bounceInLeft delay-200">
                                            <label>Name<span>*</span></label>
                                            <input type="text" class="form-control" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="form-group wow bounceInLeft delay-100">
                                            <label>email<span>*</span></label>
                                            <input type="email" class="form-control" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="form-group wow bounceInLeft delay-300">
                                            <label>phone<span>*</span></label>
                                            <input type="text" class="form-control" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="form-group wow bounceInLeft delay-400">
                                            <label>subject<span>*</span></label>
                                            <input type="text" class="form-control" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <div class="form-group wow bounceInLeft delay-500">
                                            <label>Message<span>*</span></label>
                                            <textarea class="form-control" required></textarea>
                                        </div>
                                    </div>
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <div class="form-group wow bounceInLeft delay-100">
                                            <button type="submit" class="btn btn-danger">send massage</button>
                                        </div>
                                    </div>
                                </div>
                            </form>

                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="contact-map wow bounceInRight delay-100">
                            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3206999.9229094028!2d-94.68036438842559!3d38.28329754183497!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x87c0e6d54822e37d%3A0xb55bc4590466bd72!2sMissouri%2C+USA!5e0!3m2!1sen!2sin!4v1488516820426"
                                    style="border:0" allowfullscreen=""></iframe>
                        </div>
                    </div>
                </div>

                <div class="contact-info">
                    <div class="row">
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                            <div class="cont-item wow bounceInUp delay-100">
                                <div class="icon">
                                    <i class="fa fa-phone"></i>
                                </div>
                                <div class="content-item">
                                    <h1>our headquarters</h1>
                                    <p>1/3 el-lasilky st:palasten square, maadi, cairo, egypt</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                            <div class="cont-item wow bounceInUp delay-200">
                                <div class="icon">
                                    <i class="fa fa-phone"></i>
                                </div>
                                <div class="content-item">
                                    <h1>speak to us</h1>
                                    <p>(+2)01020704551,<br> tel/fax:(+2)022-5177217 </p>

                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                            <div class="cont-item wow bounceInUp delay-300">
                                <div class="icon">
                                    <i class="fa fa-envelope"></i>
                                </div>
                                <div class="content-item">
                                    <h1>email</h1>
                                    <p>1info@example.com</p>
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="services">

        <div class="container">
            <div class="contentser">
                <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-6 col-sx-12 othercol">
                        <div class="item-ser wow bounceInUp delay-100">
                            <div class="icon">
                                <i class="fa fa-truck"></i>
                            </div>
                            <div class="contenttt">
                                <p>free shipping on order over 99$</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-sx-12 othercol">
                        <div class="item-ser wow bounceInUp delay-200">
                            <div class="icon">
                                <i class="fa fa-phone"></i>
                            </div>
                            <div class="contenttt">
                                <p>have a questions? +01020704551</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-sx-12 othercol">
                        <div class="item-ser wow bounceInUp delay-300">
                            <div class="icon">
                                <i class="fa fa-dollar"></i>
                            </div>
                            <div class="contenttt">
                                <p>100% money back guarantee</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-sx-12 othercol">
                        <div class="item-ser wow bounceInUp delay-400">
                            <div class="icon">
                                <i class="fa fa-shopping-bag"></i>
                            </div>
                            <div class="contenttt">
                                <p>30 days return services</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

